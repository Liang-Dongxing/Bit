<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysThirdAccessMapper">

    <resultMap type="SysThirdAccess" id="SysThirdAccessResult">
        <result property="accessId" column="access_id"/>
        <result property="accessKeySecret" column="access_key_secret"/>
        <result property="accessKeyId" column="access_key_id"/>
        <result property="accessKeyType" column="access_key_type"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectSysThirdAccessVo">
        select access_id,
               access_key_secret,
               access_key_id,
               access_key_type,
               create_by,
               create_time,
               update_by,
               update_time
        from sys_third_access
    </sql>

    <select id="selectSysThirdAccessList" parameterType="SysThirdAccess" resultMap="SysThirdAccessResult">
        <include refid="selectSysThirdAccessVo"/>
        <where>
            <if test="accessKeyId != null  and accessKeyId != ''">and access_key_id = #{accessKeyId}</if>
            <if test="accessKeyType != null  and accessKeyType != ''">and access_key_type = #{accessKeyType}</if>
        </where>
    </select>

    <select id="selectSysThirdAccessByAccessId" parameterType="Long" resultMap="SysThirdAccessResult">
        <include refid="selectSysThirdAccessVo"/>
        where access_id = #{accessId}
    </select>

    <insert id="insertSysThirdAccess" parameterType="SysThirdAccess" useGeneratedKeys="true" keyProperty="accessId">
        insert into sys_third_access
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="accessKeySecret != null and accessKeySecret != ''">access_key_secret,</if>
            <if test="accessKeyId != null and accessKeyId != ''">access_key_id,</if>
            <if test="accessKeyType != null and accessKeyType != ''">access_key_type,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="accessKeySecret != null and accessKeySecret != ''">#{accessKeySecret},</if>
            <if test="accessKeyId != null and accessKeyId != ''">#{accessKeyId},</if>
            <if test="accessKeyType != null and accessKeyType != ''">#{accessKeyType},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>

    <update id="updateSysThirdAccess" parameterType="SysThirdAccess">
        update sys_third_access
        <trim prefix="SET" suffixOverrides=",">
            <if test="accessKeySecret != null and accessKeySecret != ''">access_key_secret = #{accessKeySecret},</if>
            <if test="accessKeyId != null and accessKeyId != ''">access_key_id = #{accessKeyId},</if>
            <if test="accessKeyType != null and accessKeyType != ''">access_key_type = #{accessKeyType},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where access_id = #{accessId}
    </update>

    <delete id="deleteSysThirdAccessByAccessId" parameterType="Long">
        delete
        from sys_third_access
        where access_id = #{accessId}
    </delete>

    <delete id="deleteSysThirdAccessByAccessIds" parameterType="String">
        delete from sys_third_access where access_id in
        <foreach item="accessId" collection="array" open="(" separator="," close=")">
            #{accessId}
        </foreach>
    </delete>
</mapper>